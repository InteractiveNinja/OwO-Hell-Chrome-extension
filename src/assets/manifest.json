{
    "name": "OwOfie",
    "description": "A Chrome Plugin that OwOfies your Text on your sites",
    "version": "2.0.0",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": "assets/icon.png"
    },
    "permissions": 
        [
            "declarativeContent",
            "activeTab",
            "tabs",
            "<all_urls>"
        ],
    "icons": { "16": "icon.png",
        "48": "icon.png",
       "128": "icon.png" },
      "web_accessible_resources": [
        "imgs/*"
      ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "script.js"
            ],
            "run_at": "document_end",
            "all_frames": true
        }
    ]
}